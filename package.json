{
  "name": "armygametakegazillion",
  "version": "1.0.0",
  "description": "3JS Military Game - Commander-led RTS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
